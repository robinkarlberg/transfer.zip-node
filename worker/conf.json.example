{
  "active": "local",
  "providers": {
    "r2": {
      "endpoint": "https://<CLOUDFLARE_ACCOUNT_ID>.r2.cloudflarestorage.com",
      "bucket": "<BUCKET_NAME>",
      "accessKey": "<R2_KEY>",
      "secretKey": "<R2_SECRET>",
      "forcePath": true
    },
    "hetzner": {
      "endpoint": "https://<LOCATION>.your-objectstorage.com",
      "bucket": "<BUCKET_NAME>",
      "accessKey": "<HETZNER_KEY>",
      "secretKey": "<HETZNER_SECRET>"
    },
    "local": {
      "root": "./uploads"
    }
  },
  "partSizeMB": 64,
  "lockTTL": 900000
}